# This file was generated by Ansible for {{ ansible_fqdn }}
# Do NOT modify this file by hand!

{% macro render(source, options) %}
{{source}} {
{% for name, value in options.items() %}
{% if name != '_condition' %}
        {{ name }} => {{ value|to_nice_json }}
{% endif %}
{% endfor %}
    }
{% endmacro %}

output {
{% for options in logstash_config_outputs %}{% for name, values in options.items() %}
{% if values.get('_condition') %}
    {{ values['_condition'] }} {
        {{ render(name, values) }}
    }
{% else %}
    {{ render(name, values) }}
{% endif %}
{% endfor %}{% endfor %}
}
